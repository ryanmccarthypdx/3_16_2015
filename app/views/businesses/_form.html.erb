<%= render "layouts/errors", :object => @business %>

<%= form_for @business do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <%= f.label :website %>
  <%= f.text_field :website %>
  <%= f.submit %>
<% end %>
